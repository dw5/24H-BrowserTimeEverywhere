{
  "name": "Force 24-Hour Time Format EVERYWHERE",
  "description": "Displays time as 24-Hour instead of confusing am/pm",
  "homepage_url": "https://github.com/dw5/24H-BrowserTimeEverywhere",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 3,
  "version": "2.0.0.1",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": [
        "12hto24h.user.js"
      ]
    }
  ]
}
